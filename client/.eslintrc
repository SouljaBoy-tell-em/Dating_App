{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    // "no-console": "warn",
    "quotes": ["warn", "double"],
    "jsx-quotes": ["warn", "prefer-double"],
    "prefer-const": "error",
    "indent": ["warn", 2],
    "max-len": ["error", {"code": 120}],
    "semi": ["warn", "always"],
    "import/order": ["warn", {
        "groups": ["builtin", "external", "internal", "parent", "sibling", "index", "object", "type"],
        "newlines-between": "always-and-inside-groups"
    }]
  }
}
